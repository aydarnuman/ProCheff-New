{
  "name": "procheff",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export:full": "npm run build && cp public/sitemap.xml public/robots.txt public/manifest.json public/sw.js out/",
    "build:github-pages": "NODE_ENV=production next build",
    "deploy:pages": "GITHUB_PAGES=true npm run export:full && touch out/.nojekyll",
    "start": "node .next/standalone/server.js",
    "type-check": "tsc --noEmit",
    "ci": "npm ci && npm run type-check && npm run build",
    "dev:clean": "rm -rf .next && npm run dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev:analyze": "ANALYZE=true npm run build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:standalone": "npm run build && echo 'Standalone build ready!'",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules package-lock.json && npm install",
    "api:test": "node scripts/test-apis.js",
    "api:docs": "node scripts/generate-api-docs.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:integration:production": "NODE_ENV=production vitest run --config vitest.integration.config.ts",
    "test:server:start": "node scripts/test-server-manager.js start",
    "test:server:stop": "node scripts/test-server-manager.js stop",
    "test:server:cleanup": "node scripts/test-server-manager.js cleanup",
    "preview:start": "node scripts/local-preview-manager.js start",
    "preview:stop": "node scripts/local-preview-manager.js stop",
    "preview:hot-reload-test": "node scripts/hot-reload-tester.js",
    "preview:quick": "node scripts/quick-local-preview.js start",
    "preview:quick:stop": "node scripts/quick-local-preview.js stop",
    "design:drift": "node scripts/design-drift-audit.js",
    "design:update": "node scripts/design-system-updater.js",
    "claude:fallback:test": "node scripts/claude-fallback-manager.js test",
    "claude:fallback:create": "node scripts/claude-fallback-manager.js create-route",
    "test:watch": "vitest watch",
    "db:seed": "node scripts/seed-database.js",
    "db:backup": "node scripts/backup-database.js",
    "deploy:vercel": "vercel --prod",
    "deploy:vercel:preview": "vercel",
    "deploy:staging": "npm run build && npm run deploy:vercel:preview",
    "deploy:prod": "npm run build && npm run deploy:vercel",
    "health": "curl -f http://localhost:3000/api/health || exit 1",
    "monitor": "node scripts/monitor.js",
    "nightly": "node scripts/nightly-report.js",
    "nightly:view": "node scripts/view-reports.js",
    "nightly:latest": "node scripts/view-reports.js latest",
    "nightly:today": "node scripts/view-reports.js today",
    "nightly:history": "ls -la reports/ | grep nightly-report",
    "nightly:setup": "./scripts/setup-nightly.sh",
    "nightly:monthly": "node scripts/monthly-summary.js",
    "nightly:flakey": "node scripts/flakey-quarantine.js",
    "nightly:slo": "node scripts/slo-monitor.js",
    "nightly:exceptions": "node scripts/exception-rules.js",
    "nightly:privacy": "node scripts/privacy-scanner.js",
    "upgrade:preview": "node scripts/upgrade-preview.js",
    "upgrade:next": "node scripts/upgrade-preview.js next",
    "upgrade:tailwind": "node scripts/upgrade-preview.js tailwindcss",
    "setup": "npm install && npm run type-check"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/jspdf": "^1.3.3",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "form-data": "^4.0.4",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "next": "^14.2.33",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.9",
    "pdf-parse": "^1.1.1",
    "prisma": "^6.17.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "recharts": "^3.3.0",
    "roboto-fontface": "^0.10.0",
    "tailwind-merge": "^3.3.1",
    "tesseract.js": "^6.0.1",
    "zod": "3.23.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@types/node": "^20.19.22",
    "@types/node-fetch": "^2.6.13",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "8.57.1",
    "eslint-config-next": "15.5.6",
    "jsdom": "^27.0.0",
    "node-fetch": "^2.7.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  }
}
